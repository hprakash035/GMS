<mvc:View
    controllerName="gmsfiori.controller.Initial_ItemConfig"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:t="sap.ui.table"
    xmlns:tnt="sap.tnt"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
>
    <Shell id="shell">
        <App id="app">
            <pages>
                <Page
                    id="page1"
                    title="{i18n>title}"

                >
                    <content>
                        <ComboBox
                            id="comboBox"
                            width="20%"
                            selectedKey="GSA" 
                            change="onComboBoxChange"
                            
                        >
                            <items>
                                <core:Item
                                    key="GSA"
                                    text="GSA"
                                />
                                <core:Item
                                    key="GTA"
                                    text="GTA"
                                />
                            </items>
                        </ComboBox>
                        <Table
                            items="{/ServiceParameter}"
                            id="table0"
                            mode="SingleSelectLeft"
                            selectionChange="onSelect"
                           
                            
                        >
                            <headerToolbar>
                           
                                <OverflowToolbar id="_IDGenOverflowToolbar1">
                                    <content>
                                     <Text text="GSA"></Text>
                                        <ToolbarSpacer
                                            id="_IDGenToolbarSpacer1"
                                        />
                                        <SearchField
                                            id="searchField"
                                            width="20%"
                                            placeholder="search"
                                            search=".onSearch"
                                        />
                                         <Button
                                            id="gsaSaveButton"
                                            text="Save"
                                            press="gsaSaveButton"
                                        >
                                            <layoutData>
                                                <OverflowToolbarLayoutData
                                                    id="_IDGenOverflowToolbarLayoutData111"
                                                    priority="NeverOverflow"
                                                />
                                            </layoutData>
                                        </Button>
                                        
                                        <Button
                                            id="createButton"
                                            icon="sap-icon://add"
                                            tooltip="Create"
                                            visible="true"
                                            press="onOpenAddDialog"
                                        >
                                            <layoutData>
                                                <OverflowToolbarLayoutData
                                                    id="_IDGenOverflowToolbarLayoutData1"
                                                    priority="NeverOverflow"
                                                />
                                            </layoutData>
                                        </Button>
                                        <Button
                                            id="deleteButton"
                                            icon="sap-icon://delete"
                                            tooltip="Delete"
                                            visible="false"
                                            press="onDelete"
                                        >
                                            <layoutData>
                                                <OverflowToolbarLayoutData
                                                    id="_IDGenOverflowToolbarLayoutData2"
                                                    priority="NeverOverflow"
                                                />
                                            </layoutData>
                                        </Button>

                                        <Button
                                            id="saveButton"
                                            text="Save"
                                            type="Emphasized"
                                            visible="false"
                                            enabled="true"
                                            press="onSave"
                                        />
                                        <Button
                                            id="editModeButton"
                                            visible="true"
                                            icon="sap-icon://edit"
                                            tooltip="Edit"
                                            press="onEditMode"
                                        >
                                            <layoutData>
                                                <OverflowToolbarLayoutData
                                                    id="_IDGenOverflowToolbarLayoutData3"
                                                    priority="NeverOverflow"
                                                />
                                            </layoutData>
                                        </Button>
                                    </content>
                                    <dependents>
                                        <Dialog
                                            id="OpenDialog"
                                            title="Create Sales Order"
                                        >
                                            <buttons>
                                                <Button
                                                    id="confirmCreate"
                                                    text="Create"
                                                    press=".onCreate"
                                                    type="Emphasized"
                                                />
                                                <Button
                                                    id="cancelCreate"
                                                    text="Cancel"
                                                    press="onCancelDialog"
                                                    type="Transparent"
                                                />
                                            </buttons>
                                            <form:SimpleForm
                                                id="_IDGenSimpleForm1"
                                                editable="true"
                                                layout="ResponsiveGridLayout"
                                            >
                                                <form:content>
                                                    <Label
                                                        id="_IDGenLabel1"
                                                        text="ID"
                                                        required="true"
                                                    />
                                                    <Input
                                                        id="idSo"
                                                        change="onNameChange"
                                                    />
                                                    <Label
                                                        id="_IDGenLabel2"
                                                        text="Service Parameter"
                                                    />
                                                    <Input id="idCustName" />
                                                </form:content>
                                            </form:SimpleForm>
                                        </Dialog>
                                    </dependents>
                                </OverflowToolbar>
                            </headerToolbar>
                            <items>
                                <ColumnListItem
                                    type="Active"
                                    id="item0"
                                >
                                    <cells>
                                        <Text
                                            id="id1"
                                            text="{ServiceParameter_ID}"
                                        />
                                        <Text
                                            id="id2"
                                            text="{Service_Parameters}"
                                        />
                                        <CheckBox id="chxbox" />
                                        <CheckBox id="che" />

                                        <Button
                                            id="id6"
                                            icon="sap-icon://edit"
                                            press="onSelect"
                                        />
                                    </cells>
                                </ColumnListItem>
                            </items>
                            <columns>
                            
                                <Column id="_IDGenColumn1">
                                    <header>
                                        <Label
                                            id="_IDGenLabel6"
                                            text="ID"
                                        />
                                    </header>
                                </Column>
                                <Column id="_IDGenColumn2">
                                    <header>
                                        <Label
                                            id="_IDGenLabel7"
                                            text="Service Parameter"
                                        />
                                    </header>
                                </Column>
                                <Column id="_IDGenColun3">
                                    <header>
                                        <Label
                                            id="_IDGenLab7"
                                            text="Initial Page"
                                        />
                                    </header>
                                </Column>
                                <Column id="_IDGenColun4">
                                    <header>
                                        <Label
                                            id="_IDGenLab8"
                                            text="Items Page"
                                        />
                                    </header>
                                </Column>
                            </columns>
                        </Table>
                    <Table
                            items="{/ServiceParameter}"
                            id="table1"
                            mode="SingleSelectLeft"
                            selectionChange="onSelect"
                            visible="false"
                            
                        >
                            <headerToolbar>
                                <OverflowToolbar id="_IDGenOverflowToolbar11">
                                    <content>
                                        <Text text="GTA"></Text>
                                        <ToolbarSpacer
                                            id="_IDGenToolbarSpacer11"
                                        />
                                        
                                        <SearchField
                                            id="searchField1"
                                            width="20%"
                                            placeholder="search"
                                            search=".onSearch"
                                        />
                                         <Button
                                            id="gtaSaveButton"
                                            text="Save"
                                            press="gtaSaveButton"
                                        >
                                            <layoutData>
                                                <OverflowToolbarLayoutData
                                                    id="_IDGenOverflowToolbarLayoutData01"
                                                    priority="NeverOverflow"
                                                />
                                            </layoutData>
                                        </Button>
                                        
                                        <Button
                                            id="createButton1"
                                            icon="sap-icon://add"
                                            tooltip="Create"
                                            visible="true"
                                            press="onOpenAddDialog"
                                        >
                                            <layoutData>
                                                <OverflowToolbarLayoutData
                                                    id="_IDGenOverflowToolbarLayoutData11"
                                                    priority="NeverOverflow"
                                                />
                                            </layoutData>
                                        </Button>
                                        <Button
                                            id="deleteButton1"
                                            icon="sap-icon://delete"
                                            tooltip="Delete"
                                            visible="false"
                                            press="onDelete"
                                        >
                                            <layoutData>
                                                <OverflowToolbarLayoutData
                                                    id="_IDGenOverflowToolbarLayoutData21"
                                                    priority="NeverOverflow"
                                                />
                                            </layoutData>
                                        </Button>

                                        <Button
                                            id="saveButton1"
                                            text="Save"
                                            type="Emphasized"
                                            visible="false"
                                            enabled="true"
                                            press="onSave"
                                        />
                                        <Button
                                            id="editModeButton1"
                                            visible="true"
                                            icon="sap-icon://edit"
                                            tooltip="Edit"
                                            press="onEditMode"
                                        >
                                        
                                            <layoutData>
                                                <OverflowToolbarLayoutData
                                                    id="_IDGenOverflowToolbarLayoutData31"
                                                    priority="NeverOverflow"
                                                />
                                            </layoutData>
                                        </Button>
                                    </content>
                                    <dependents>
                                        <Dialog
                                            id="OpenDialog1"
                                            title="Create Sales Order"
                                        >
                                            <buttons>
                                                <Button
                                                    id="confirmCreate1"
                                                    text="Create"
                                                    press=".onCreate"
                                                    type="Emphasized"
                                                />
                                                <Button
                                                    id="cancelCreate1"
                                                    text="Cancel"
                                                    press="onCancelDialog"
                                                    type="Transparent"
                                                />
                                            </buttons>
                                            <form:SimpleForm
                                                id="_IDGenSimpleForm11"
                                                editable="true"
                                                layout="ResponsiveGridLayout"
                                            >
                                                <form:content>
                                                    <Label
                                                        id="_IDGenLabel11"
                                                        text="ID"
                                                        required="true"
                                                    />
                                                    <Input
                                                        id="idSo1"
                                                        change="onNameChange"
                                                    />
                                                    <Label
                                                        id="_IDGenLabel21"
                                                        text="Service Parameter"
                                                    />
                                                    <Input id="idCustName1" />
                                                </form:content>
                                            </form:SimpleForm>
                                        </Dialog>
                                    </dependents>
                                </OverflowToolbar>
                            </headerToolbar>
                            <items>
                                <ColumnListItem
                                    type="Active"
                                    id="item01"
                                >
                                    <cells>
                                        <Text
                                            id="id11"
                                            text="{ServiceParameter_ID}"
                                        />
                                        <Text
                                            id="id21"
                                            text="{Service_Parameters}"
                                        />
                                        <CheckBox id="chxbox1" />
                                        <CheckBox id="che1" />

                                        <Button
                                            id="id61"
                                            icon="sap-icon://edit"
                                            press="onSelect"
                                        />
                                    </cells>
                                </ColumnListItem>
                            </items>
                            <columns>
                            
                                <Column id="_IDGenColumn11">
                                    <header>
                                        <Label
                                            id="_IDGenLabel61"
                                            text="ID"
                                        />
                                    </header>
                                </Column>
                                <Column id="_IDGenColumn21">
                                    <header>
                                        <Label
                                            id="_IDGenLabel71"
                                            text="Service Parameter"
                                        />
                                    </header>
                                </Column>
                                <Column id="_IDGenColun31">
                                    <header>
                                        <Label
                                            id="_IDGenLab17"
                                            text="Initial Page"
                                        />
                                    </header>
                                </Column>
                                <Column id="_IDGenColun41">
                                    <header>
                                        <Label
                                            id="_IDGenLab81"
                                            text="Items Page"
                                        />
                                    </header>
                                </Column>
                            </columns>
                        </Table>

                    </content>
                </Page>
                
            </pages>
        </App>
    </Shell>
</mvc:View>
